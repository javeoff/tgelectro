{
  "ts-node": {
    "files": true,
    "require": [
      "tsconfig-paths/register"
    ],
    "transpileOnly": true
  },
  "compilerOptions": {
    "paths": {
      "@assets/*": [
        "src/assets/*"
      ],
      "@common/*": [
        "src/common/*"
      ],
      "@pages/*": [
        "src/pages/*"
      ],
      "@server/*": [
        "src/server/*"
      ],
      "@components/*": [
        "src/components/*"
      ],
      "@styled/*": [
        "src/styled/*"
      ],
      "react-native-sqlite-storage": [
        "src/dummy.ts"
      ]
    },
    "baseUrl": ".",
    "target": "esnext",
    "module": "commonjs",
    "moduleResolution": "node",
    "noImplicitAny": false,
    "jsx": "preserve",
    "sourceMap": true,
    "outDir": "dist",
    "rootDir": ".",
    "types": [
      "node",
    ],
    "lib": [
      "ESNext",
      "dom",
      "DOM.Iterable"
    ],
    "allowSyntheticDefaultImports": true,
    "downlevelIteration": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "keyofStringsOnly": true,
    "emitDecoratorMetadata": true,
    "allowJs": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "isolatedModules": true,
    "skipLibCheck": true,
    "strict": true,
    "incremental": true,
  },
  "include": [
    "src/**/*",
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx"
  ],
  "exclude": [
    "node_modules"
  ]
}
